{
  "name": "ms-notifier-js",
  "version": "2.0.0",
  "description": "MC-Notifier-JS",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsup",
    "dev": "pnpm with-env tsup --watch --onSuccess \"node dist/index.mjs\"",
    "start": "pnpm with-env node dist/index.mjs",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "format:fix": "prettier --check --write . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "typecheck": "tsc --noEmit",
    "precommit": "pnpm format:fix && pnpm lint:fix && pnpm typecheck",
    "with-env": "dotenv -e ./.env --"
  },
  "dependencies": {
    "discord.js": "^14.25.1",
    "minecraft-server-util": "^5.4.4",
    "remeda": "^2.33.1",
    "systeminformation": "^5.28.10",
    "yaml": "^2.8.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.1",
    "@eslint/js": "^9.39.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@types/node": "^22.19.3",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "prettier": "^3.7.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
